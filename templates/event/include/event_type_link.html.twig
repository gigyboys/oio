<span class="et_type_item {% if typeslug == 'upcoming' %}active{% endif %}">
	<a href="{{ path('event') }}"><span>En cours / à venir</span></a>
</span>
<span class="et_type_item {% if typeslug == 'passed' %}active{% endif %}">
	<a href="{{ path('event', {'typeslug':'passed'}) }}"><span class="hidden_530">Evènements</span> <span>Passés</span></a>
</span>
<span class="et_type_item {% if typeslug == 'all' %}active{% endif %}">
	<a href="{{ path('event', {'typeslug':'all'}) }}"><span>Touts</span> <span class="hidden_530">Evènements</span></a>
</span>
<span class="dd d_ib">
	<span href="#" class="dd_src c_p">
		{% if tag is defined %}
			<span>{{ tag.name }}</span>
		{% else %}
			<span style="color:#999">Tags</span>
		{% endif %}
		
	</span>
	
	<span class="dd_target list d_n" style="position: absolute; background:#eee; padding:5px; z-index:5; border:1px solid #ccc" data-display="block">
		<span class="mg_5 d_b">
			{% set tags = getTagsWithPublishedEvent() %}
				{% for tagTemp in tags %}
					<span class="d_b">
						<a class="et_tag" href="{{ path('event_view_tag', {'slug': tagTemp.slug}) }}">
							{{ tagTemp.name }} 
							{{ getPublishedEventsByTag(tagTemp) | length }}
						</a>
					</span>
				{% endfor %}
		</span>
	</span>
</span>